{
  "name": "shaymaa-abdou-capstone-server",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "db:migrate": "knex migrate:latest",
    "db:rollback": "knex migrate:rollback --all",
    "db:seed": "knex seed:run",
    "db:reset": "npm run db:rollback && npm run db:migrate && npm run db:seed"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "knex": "^2.4.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.4.0",
    "pg": "^8.11.0"
  }
}
